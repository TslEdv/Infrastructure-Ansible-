$TTL	604800
{{ startup }}.ee.	IN	SOA	TslEdv1.{{ startup }}.ee. edvin.{{ startup }}.ee. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
{{ startup }}.ee.	IN	NS	TslEdv1
{% for vm in groups ['all'] %}
{{ vm }} IN A {{ hostvars[vm]['ansible_default_ipv4']['address'] }}
{% endfor %}


